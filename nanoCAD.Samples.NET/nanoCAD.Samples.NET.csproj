        <Compile Include="..\src\Service\AsmInfo.cs" Link="AsmInfo.cs" />
        <Compile Include="..\src\CadCommand.cs" Link="CadCommand.cs" />
        <Compile Include="..\src\Service\CmdInfo.cs" Link="CmdInfo.cs" />
        <Compile Include="..\src\Service\Msg.cs" Link="Msg.cs" />
    </ItemGroup>-->
</Project>
